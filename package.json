{
  "name": "digital-samba-mcp",
  "version": "0.1.0",
  "description": "Digital Samba MCP Server - Model Context Protocol server for Digital Samba's video conferencing API",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "digital-samba-mcp": "./dist/bin/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./server": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./client": {
      "import": "./dist/src/digital-samba-api.js",
      "types": "./dist/src/digital-samba-api.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "PACKAGE.md",
    "TROUBLESHOOTING.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --skipLibCheck",
    "build:clean": "rimraf dist && tsc --skipLibCheck",
    "start": "node dist/src/index.js",
    "dev": "tsx --inspect src/index.ts",
    "dev:watch": "tsx --watch --inspect src/index.ts",
    "debug": "node --inspect node_modules/.bin/tsx src/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"bin/**/*.js\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:webhook": "node tests/webhook-test.js",
    "test:server": "node tests/test-mcp-server.js",
    "test:recording": "node tests/recording-test.js",
    "test:moderation": "node tests/moderation-test.js",
    "test:breakout": "node tests/test-breakout-rooms.js",
    "test:meetings": "node tests/meeting-scheduling-test.js",
    "prepublishOnly": "npm run build:clean",
    "postbuild": "node -e \"const fs = require('fs'); const path = require('path'); const srcPath = path.join(process.cwd(), 'bin', 'cli.js'); const destPath = path.join(process.cwd(), 'dist', 'bin', 'cli.js'); require('fs').mkdirSync(path.dirname(destPath), { recursive: true }); fs.copyFileSync(srcPath, destPath); fs.chmodSync(destPath, '755');\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.4",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "digital-samba",
    "video-conferencing",
    "llm",
    "claude",
    "anthropic"
  ],
  "author": "Digital Samba",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-samba/digital-samba-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/digital-samba/digital-samba-mcp-server/issues"
  },
  "homepage": "https://github.com/digital-samba/digital-samba-mcp-server#readme",
  "sideEffects": false
}
