{
  "name": "digital-samba-mcp",
  "version": "0.1.0",
  "description": "Digital Samba MCP Server - Model Context Protocol server for Digital Samba's video conferencing API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "digital-samba-mcp": "./dist/bin/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "PACKAGE.md",
    "TROUBLESHOOTING.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --skipLibCheck",
    "build:clean": "rimraf dist && tsc --skipLibCheck",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx --watch src/index.ts",
    "lint": "eslint src --ext .ts",
    "test:webhook": "node tests/webhook-test.js",
    "test:server": "node tests/test-mcp-server.js",
    "test:recording": "node tests/recording-test.js",
    "test:moderation": "node tests/moderation-test.js",
    "test:breakout": "node tests/test-breakout-rooms.js",
    "prepublishOnly": "npm run build:clean",
    "postbuild": "cp bin/cli.js dist/bin/cli.js && chmod +x dist/bin/cli.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.4",
    "express": "^4.18.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "digital-samba",
    "video-conferencing",
    "llm",
    "claude",
    "anthropic"
  ],
  "author": "Digital Samba",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-samba/digital-samba-mcp-server.git"
  }
}
